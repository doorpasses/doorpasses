{
  "name": "csharp-sdk",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/csharp-sdk/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "options": {
        "command": "/opt/homebrew/opt/dotnet@8/bin/dotnet build DoorPasses.sln --configuration Release",
        "cwd": "packages/csharp-sdk"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "command": "/opt/homebrew/opt/dotnet@8/bin/dotnet test DoorPasses.Tests.csproj --logger \"console;verbosity=detailed\"",
        "cwd": "packages/csharp-sdk"
      }
    },
    "pack": {
      "executor": "nx:run-commands",
      "options": {
        "command": "/opt/homebrew/opt/dotnet@8/bin/dotnet pack DoorPasses.csproj -c Release -o ./dist",
        "cwd": "packages/csharp-sdk"
      },
      "dependsOn": [
        "build"
      ]
    },
    "clean": {
      "executor": "nx:run-commands",
      "options": {
        "command": "/opt/homebrew/opt/dotnet@8/bin/dotnet clean DoorPasses.sln",
        "cwd": "packages/csharp-sdk"
      }
    }
  },
  "tags": [
    "sdk",
    "csharp"
  ]
}
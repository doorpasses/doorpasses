{
  "type": "module",
  "name": "@repo/database",
  "version": "0.0.0",
  "private": true,
  "exports": {
    ".": {
      "types": "./db.server.ts",
      "import": "./db.server.ts",
      "default": "./db.server.ts"
    },
    "./types": {
      "types": "./types.ts",
      "import": "./types.ts",
      "default": "./types.ts"
    }
  },
  "scripts": {
    "db:generate": "npx varlock run -- prisma generate --sql",
    "db:push": "npx varlock run -- prisma db push --skip-generate",
    "db:seed": "npx varlock run -- tsx seed.ts",
    "db:migrate:dev": "npx varlock run -- prisma migrate dev",
    "db:migrate:deploy": "npx varlock run -- prisma migrate deploy",
    "setup": "npm run db:migrate:deploy && npm run db:generate"
  },
  "dependencies": {
    "@epic-web/remember": "^1.1.0",
    "@prisma/client": "^6.18.0",
    "@repo/auth": "*",
    "@repo/test-utils": "*"
  },
  "devDependencies": {
    "varlock": "^0.1.0",
    "@faker-js/faker": "^9.7.0",
    "prisma": "^6.8.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}

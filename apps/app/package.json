  "scripts": {
    "build": "run-s build:*",
    "build:remix": "react-router build",
    "build:server": "tsx ./other/build-server.ts",
    "dev": "cross-env NODE_ENV=development MOCKS=true DISABLE_STRIPE_MOCKS=false tsx ./index.js",
    "dev:no-mocks": "cross-env NODE_ENV=development node ./server/dev-server.js",
    "dev:all-mocks": "cross-env NODE_ENV=development MOCKS=true tsx ./index.js",
    "format": "prettier --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:oxc": "oxlint .",
    "lint:oxc:fix": "oxlint --fix .",
    "lint:all": "oxlint . && eslint .",
    "setup": "npm run build && npm run db:migrate:deploy && npm run db:generate && playwright install",
    "db:generate": "cd ../../packages/database && npm run db:generate",
    "db:migrate:deploy": "cd ../../packages/database && npm run db:migrate:deploy",
    "db:seed": "cd ../../packages/database && npm run db:seed",
    "db:migrate:dev": "cd ../../packages/database && npm run db:migrate:dev",
    "start": "cross-env NODE_ENV=production node .",
    "start:mocks": "cross-env NODE_ENV=production MOCKS=true tsx .",
    "test": "vitest",
    "test:mcp": "npx @modelcontextprotocol/inspector",
    "coverage": "vitest run --coverage",
    "test:e2e": "npm run test:e2e:dev --silent",
    "test:e2e:dev": "playwright test --ui",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env CI=true playwright test",
    "test:e2e:waitlist": "cross-env LAUNCH_STATUS=CLOSED_BETA playwright test waitlist-referral",
    "test:e2e:waitlist:ui": "cross-env LAUNCH_STATUS=CLOSED_BETA playwright test waitlist-referral --ui",
    "typecheck": "react-router typegen && tsc",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run",
    "lingui:extract": "lingui extract"
  },
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/playwright-report",
    "/server-build"
  ],
